




<tal:sections tal:define="portal_tabs view/portal_tabs"
     tal:condition="portal_tabs"
     i18n:domain="plone">
    <p class="hiddenStructure" i18n:translate="heading_sections">Navigation</p>



        <ul id="navmenu-primary-list" tal:define="selected_tab python:view.selected_portal_tab">
      <tal:tabs tal:repeat="tab python:portal_tabs[:7]">


          <li
            tal:define="tid tab/id; name tab/name; submenu tab/navmenucode" tal:attributes="id string:navitem-$tid" 
            id="navitem-topics"  class="submenu-item-wrapper" tal:condition="python:name!='Home'">

            <div class="submenu">

              <div class="submenu-item" tal:attributes="id string:portaltab-${tid};">
                <a href=""
              tal:content="tab/name"
              tal:attributes="href tab/url;
              title tab/description|nothing;">
              Tab Name
            </a>

                <i class="fa fa-angle-down" class="submenu-button" data-toggle="navmenu" data-target="#submenu-dropdown"></i>
              </div>




              <div id="submenu-dropdown" class="submenu-dr-wrapper">
                <div class="submenu-dr" tal:condition="submenu">

                  <div class="dr-item" tal:replace="structure submenu" />
          

                  
                 
                </div>
              </div>
            </div>
          </li>


         
     </tal:tabs>
        </ul>
         




    

<div id="backdrop"></div>



</tal:sections>